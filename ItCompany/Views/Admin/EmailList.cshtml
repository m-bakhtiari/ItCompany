@model List<Email>
@{
    Layout = "_AdminLayout";
    var i = 1;
}
<section id="main-content">
    <section class="wrapper">
        <!-- page start-->
        <div class="row">
            <div class="col-sm-12">
                <section class="panel">
                    <header class="panel-heading">
                        جدول
                    </header>
                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>آدرس ایمیل</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@i</td>
                                    <td>@item.EmailAddress</td>
                                </tr>
                                i++;
                            }

                        </tbody>
                    </table>
                </section>
            </div>
        </div>

        <!-- page end-->
    </section>
</section>